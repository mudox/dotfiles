{
  "title": "When Left Fn is Missing",
  "rules": [
    {
      "description": "Fn + Delete -> Ctrl + Delete",
      "manipulators": [
        {
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "fn"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
